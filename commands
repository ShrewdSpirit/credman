
- add
    - profile <"profile name">
    - site <"site"> [field flags="value"] [-p/-profile="profile name"] [password options]

- del/delete
    - profile <"profile name">
    - site <"site"> [-p/-profile="profile name"]

- ren/rename
    - profile <"profile name"> <"new name">
    - site <"site"> <"new name"> [-p/-profile="profile name"]

- passwd
    - profile <"profile name">
    - site <"site"> [-p/-profile="profile name"] [password options]

- edit [edits a site] <"site"> <field flags="new value"> [-p/-profile="profile name"]

- get [gets a specific field of a site] <"site"> <field flags> [-p/-profile="profile name"]

- list
    - sites [-p/-profile="profile name"]
    - profiles

- default [sets default profile] <"profile">

- remote
    - set <"address"> <--password="remote password"> [-p/-profile="profile name"]
    - sync [-p/-profile="profile name"]

- serve


password options:
- if none is given, credman prompts user for password
- --pgen
- --plen=number
- --pcase=lower/upper/both
- --pmix=letter/digit/both
- default: --plen=16 --pcase=both --pmix=both
